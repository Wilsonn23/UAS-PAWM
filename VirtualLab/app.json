{
  "expo": {
    "name": "VirtualLab",
    "slug": "VirtualLab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Virtual_Lab_Logo.png",
    "scheme": "virtuallab",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.losiento.virtuallab",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.losiento.virtuallab",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/Virtual_Lab_Logo.png",
        "backgroundImage": "./assets/images/Virtual_Lab_Logo.png",
        "monochromeImage": "./assets/images/Virtual_Lab_Logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Virtual_Lab_Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Virtual_Lab_Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9dfc43e5-346f-42d8-ae47-a79cc8f2efef"
      }
    }
  }
}
